{
    "version": 2,
    "builds": [
      { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "app.js" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src * 'unsafe-inline' 'unsafe-eval'; font-src 'self' https://dance-website-three.vercel.app/; script-src 'self' https://vercel.live/ 'unsafe-inline';"
          }
        ]
      }
    ]
  }